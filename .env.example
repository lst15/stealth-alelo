# URL inicial para acessar antes do redirect
LOGIN_URL=https://example.com/login

# Credenciais
CPF=00000000000
SENHA=minha-senha

# Valida texto presente na pagina de destino do redirect (passo 1)
REDIRECT_VALIDATION_TEXT=Faca seu login

# Valida texto apos clicar em ENTRAR (passo 3)
POST_LOGIN_VALIDATION_TEXT=Bem-vindo

# Selectors (CSS) para os campos/botao
CPF_SELECTOR=input[name="cpf"], input[id*="cpf" i], input[placeholder*="CPF" i]
SENHA_SELECTOR=input[type="password"], input[name*="senha" i], input[id*="senha" i]
ENTRAR_BUTTON_SELECTOR=button, input[type="submit"], a
ENTRAR_BUTTON_TEXT=ENTRAR
RECEIVE_CODE_BUTTON_SELECTOR=button, input[type="button"], input[type="submit"], a
RECEIVE_CODE_BUTTON_TEXT=Receber código por EMAIL
RECEIVE_CODE_VALIDATION_TEXT=Código enviado por e-mail
OTP_INPUT_SELECTOR=input[placeholder*="Digite o código" i], input[name*="codigo" i], input[id*="codigo" i], input[type="text"]
AUTHENTICATE_BUTTON_SELECTOR=button, input[type="button"], input[type="submit"], a
AUTHENTICATE_BUTTON_TEXT=AUTENTICAR
STEP5_VALIDATION_TEXT=
# Compatibilidade antiga:
POST_AUTH_VALIDATION_TEXT=

# Caminho local onde o repo do stealth-browser-mcp sera clonado
STEALTH_BROWSER_MCP_PATH=.stealth-browser-mcp

# Modo do navegador
HEADLESS=false
BACKGROUND_HEADED=false
BROWSER_SANDBOX=false
KEEP_BROWSER_OPEN=true
USER_DATA_DIR=.browser-profile

# Timeouts/intervalo
REDIRECT_TIMEOUT_SECONDS=60
POST_LOGIN_TIMEOUT_SECONDS=60
RECEIVE_CODE_TIMEOUT_SECONDS=60
POST_AUTH_TIMEOUT_SECONDS=60
POLL_INTERVAL_SECONDS=1.0
